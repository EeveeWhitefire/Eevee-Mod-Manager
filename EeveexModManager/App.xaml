<Application x:Class="EeveexModManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EeveexModManager"
             xmlns:properties="clr-namespace:EeveexModManager.Properties"
             Startup="App_Startup"
             Exit="ApplicationExit">
    <Application.Resources>
        <ResourceDictionary>
            <properties:Settings x:Key="Settings" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/EeveexModManager;component/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
